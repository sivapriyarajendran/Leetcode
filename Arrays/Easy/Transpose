class Solution {
    public int[][] transpose(int[][] A) {
        
        int [][] result = new int[A[0].length][A.length];
        for(int i=A.length-1;i>=0;i--){
            for(int j=A[0].length-1;j>=0;j--){
                result[j][i]=A[i][j];
            }
        }
        return result;
    }
}
